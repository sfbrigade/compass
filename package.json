    "@trpc/server": "^10.16.0",
    "axios": "^1.3.2",
    "dotenv": "16.0.3",
    "kysely": "^0.23.4",
    "next": "13.1.6",
    "next-auth": "^4.20.1",
    "pg": "^8.9.0",
    "pg-connection-string": "^2.5.0",
    "postgres-migrations": "^5.3.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "winston": "^3.8.2",
    "zapatos": "^6.1.4",
    "zod": "^3.21.4"
