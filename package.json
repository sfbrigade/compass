    "eslint": "^8.34.0",
    "eslint-config-next": "13.1.6",
    "foreman": "^3.0.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "prettier": "2.8.3",
    "tsx": "^3.12.6",
    "type-fest": "^3.5.4",
    "typescript": "^4.9.5"
  },
  "engines": {
    "node": ">=18.0.0"
